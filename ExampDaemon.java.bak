class MyThread extends Thread
{
	public void run()
	{
		System.out.println("Deamon functionality");
	}
}
class ThreadDemo
{
	public static void main(String[] args)//main Thread 
	{   
		MyThread t=new MyThread();
		t.setDaemon(true);
		t.start();

		for(int i=0;i<10;i++)
		{
			System.out.println("main thread");
		}
	
	}
}